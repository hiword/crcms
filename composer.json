{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "intervention/image": "^2.3",
        "intervention/imagecache": "^2.3",
        "jenssegers/agent": "^2.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
    		"app/tool.php",
    		"app/helper.php",
    		"modules/simon/log/src/helper.php",
    		"modules/simon/file/src/helper.php"
		],
        "psr-4": {
            "App\\": "app/",
            "Simon\\Count\\": "modules/simon/count/src",
            "Simon\\Document\\": "modules/simon/document/src",
            "Simon\\Model\\": "modules/simon/model/src",
            "Simon\\File\\": "modules/simon/file/src",
            "Simon\\Mail\\": "modules/simon/mail/src",
            "Simon\\Log\\": "modules/simon/log/src",
            "Simon\\System\\": "modules/simon/system/src",
            "Simon\\Tag\\": "modules/simon/tag/src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
